{
  "extends": [
    "config:base",
    ":disableRateLimiting",
    ":rebaseStalePrs",
    ":disableDependencyDashboard"
  ],
  "assignees": ["growse"],
  "regexManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)\\sENV .*?_VERSION=(?<currentValue>.*)\\s"
      ]
    }
  ]
}
